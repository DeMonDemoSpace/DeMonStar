<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumberProgressBar">
        <attr name="nTextColor" format="color" />
        <attr name="nTextSize" format="dimension" />
        <attr name="nMax" format="integer" />
        <attr name="nCircleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="GraphicLockView">
        <attr name="glv_point_normal" format="reference" />
        <attr name="glv_point_pressed" format="reference" />
        <attr name="glv_point_error" format="reference" />
        <attr name="glv_line_pressed" format="reference" />
        <attr name="glv_line_error" format="reference" />
        <attr name="glv_type">
            <enum name="unlock" value="0" />
            <enum name="setup" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TitleDividingLine">
        <attr name="tdl_Title" format="string" />
        <attr name="tdl_TitleSize" format="dimension" />
        <attr name="tdl_Color" format="color" />
    </declare-styleable>

    <declare-styleable name="TitleTextView">
        <attr name="ttv_Title" format="string" />
        <attr name="ttv_TitleColor" format="color" />
        <attr name="ttv_TextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="riv_radius" format="dimension" />
        <attr name="riv_type">
            <enum name="normal" value="0" />
            <enum name="circle" value="1" />
            <enum name="round" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="StrongImageView">
        <attr name="scaleType" format="enum">
            <enum name="fitAuto" value="1" />
            <enum name="fitCenter" value="2" />
        </attr>
        <attr name="animateScaleType" format="boolean" />
        <attr name="maximumScale" format="float" />
        <attr name="minimumScale" format="float" />
        <attr name="scale" format="float" />
    </declare-styleable>

    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0" />
            <enum name="y" value="1" />
            <enum name="z" value="2" />
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction" />
        <attr name="pivotY" format="fraction" />
    </declare-styleable>

    <declare-styleable name="ProgressWheel">
        <attr name="progressIndeterminate" format="boolean" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="float" />
        <attr name="barSpinCycleTime" format="integer" />
        <attr name="circleRadius" format="dimension" />
        <attr name="fillRadius" format="boolean" />
        <attr name="barWidth" format="dimension" />
    </declare-styleable>


    <declare-styleable name="TimingButton">
        <!--结束后的文字-->
        <attr name="tb_psText" format="string" />
        <attr name="tb_countText" format="string" />
        <!--计时总时长，毫秒-->
        <attr name="tb_totalTime" format="integer" />
        <!--计时时间间隔，毫秒-->
        <attr name="tb_timeInterval" format="integer" />
    </declare-styleable>

    <declare-styleable name="CornerLabelView">
        <attr name="clvPaddingTop" format="dimension" />
        <attr name="clvPaddingCenter" format="dimension" />
        <attr name="clvPaddingBottom" format="dimension" />

        <attr name="clvText1" format="string" />
        <attr name="clvText1Color" format="color" />
        <attr name="clvText1Height" format="dimension" />

        <attr name="clvText2" format="string" />
        <attr name="clvText2Color" format="color" />
        <attr name="clvText2Height" format="dimension" />

        <attr name="clvFillColor" format="color" />
        <attr name="clvFlags" format="integer">
            <flag name="none" value="0" />
            <flag name="right" value="1" />
            <flag name="bottom" value="2" />
            <flag name="triangle" value="4" />
        </attr>
    </declare-styleable>


    <declare-styleable name="SlideLayout">
        <attr name="sl_direction">
            <enum name="Right" value="0" />
            <enum name="Left" value="1" />
            <enum name="Top" value="2" />
            <enum name="Bottom" value="3" />
        </attr>
        <attr name="sl_distance" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SpaceEditText">
        <attr name="set_type" format="enum">
            <enum name="Phone" value="0" />
            <enum name="BankCard" value="1" />
            <enum name="IDCard" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="BrokenLine">
        <attr name="bl_color" format="color" />
        <attr name="bl_width" format="float" />
        <attr name="bl_direction" format="enum">
            <enum name="vertical" value="0" />
            <enum name="horizontal" value="1" />
            <enum name="top_left" value="2" />
            <enum name="top_right" value="3" />
            <enum name="bottom_left" value="4" />
            <enum name="bottom_right" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ListTabView">
        <attr name="ltv_textColor" format="color" />
        <attr name="ltv_textSize" format="dimension" />
        <attr name="ltv_textString" format="string" />
        <attr name="ltv_textWeight" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomSearchView">
        <attr name="csv_query" format="string" />
    </declare-styleable>

    <string-array name="base_color">
        <item>黑色</item>
        <item>红色</item>
        <item>黄色</item>
        <item>蓝色</item>
        <item>绿色</item>
    </string-array>

    <string-array name="base_text_size">
        <item>2</item>
        <item>4</item>
        <item>6</item>
        <item>8</item>
    </string-array>


    <declare-styleable name="DrawableTextView">
        <attr name="dt_left" format="reference" />
        <attr name="dt_bottom" format="reference" />
        <attr name="dt_right" format="reference" />
        <attr name="dt_top" format="reference" />
        <attr name="dt_width" format="dimension" />
        <attr name="dt_height" format="dimension" />
        <attr name="dt_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SatelliteMenu">
        <attr name="sm_position">
            <enum name="left_top" value="0" />
            <enum name="left_bottom" value="1" />
            <enum name="right_top" value="2" />
            <enum name="right_bottom" value="3" />
        </attr>
        <attr name="sm_radius" format="dimension" />
    </declare-styleable>


    <declare-styleable name="EasyTextView">
        <attr name="etv_title" format="string" />
        <attr name="etv_text" format="string" />
        <attr name="etv_hint" format="string" />
        <attr name="etv_isRight" format="boolean" />
        <attr name="etv_line">
            <enum name="line" value="0" />
            <enum name="dashed" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="etv_textColor" format="color" />
        <attr name="etv_titleColor" format="color" />
        <attr name="etv_textSize" format="dimension" />
        <attr name="etv_titleIcon" format="reference" />
        <attr name="etv_titleSize" format="dimension" />
        <attr name="etv_rightImg" format="reference"/>
        <attr name="etv_isCanEdit" format="boolean"/>
        <attr name="etv_isMust" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="UploadImageView">
        <attr name="uiv_tips" format="string"/>
    </declare-styleable>

    <declare-styleable name="TouchImageView">
        <attr name="zoom_enabled" format="boolean" />
    </declare-styleable>


    <declare-styleable name="MutilRadioGroup">
        <attr name="mrg_gravity" format="enum"><!--对齐方式 !-->
            <enum name="right" value="0"/>
            <enum name="left" value="1"/>
            <enum name="center" value="2"/>
        </attr>
        <attr name="mrg_horizontal_Space" format="dimension"/>
        <attr name="mrg_vertical_Space" format="dimension"/>
        <attr name="mrg_isFull" format="boolean"/>
    </declare-styleable>

</resources>
